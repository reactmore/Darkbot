{
  "name": "darkbot",
  "version": "2.0.0",
  "description": "Modular Telegram Multi-Bot Runtime powered by Teleproto",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "pm2 start src/index.js --attach --name DarkRuntime",
    "migrate": "npx sequelize-cli db:migrate",
    "test": "jest --forceExit",
    "postinstall": "test -f .env || cp .env-example .env"
  },
  "keywords": [
    "telegram",
    "teleproto",
    "bot",
    "runtime",
    "multi-bot"
  ],
  "author": "darkdeath",
  "license": "ISC",
  "dependencies": {
    "@distube/ytdl-core": "^4.14.4",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.1.2",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "image-to-pdf": "^2.0.0",
    "input": "^1.0.1",
    "mysql2": "^3.14.5",
    "pm2": "^5.3.0",
    "qrcode": "^1.5.4",
    "qs": "^6.11.2",
    "sequelize": "^6.32.0",
    "sharp": "^0.33.4",
    "simple-git": "^3.19.0",
    "sqlite3": "^5.1.6",
    "teleproto": "^1.222.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.3",
    "nodemon": "^3.1.0"
  }
}